@page "/Subcategorii/"
@page "/Subcategorii/{categoryUrl}"
@inject ISubCategoryService SubCategoryService

<PageTitle>Subcategorii | DrPrint</PageTitle>
<MudBreadcrumbs Items="_items" MaxItems="5"></MudBreadcrumbs>


<SubCategoryList />



@code {

	[Parameter]
	public string categoryUrl { get; set; } = null;


	protected override async Task OnParametersSetAsync()
	{

		await SubCategoryService.GetSubCategoryCategory(categoryUrl);
	}

	private List<BreadcrumbItem> _items = new List<BreadcrumbItem>
	{
		new BreadcrumbItem("Prima pagina", href: "/"),
		new BreadcrumbItem("Inapoi", href: "javascript:history.go(-1)"),

	};
}